<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <patterns jcr:primaryType="nt:unstructured">
        <combobox/>
        <combobox1/>
    </patterns>
    <replacement
        jcr:primaryType="nt:unstructured"
        cq:rewriteIsFinal="{Boolean}true">
        <autocomplete
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/form/autocomplete"
            fieldDescription="Enter a property like 'jcr:created'"
            fieldLabel="Order by"
            name="./orderBy">
            <options
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/autocomplete/list"/>
            <items
                cq:mapsTo="./options"
                jcr:primaryType="nt:unstructured"/>
        </autocomplete>
    </replacement>
</jcr:root>
